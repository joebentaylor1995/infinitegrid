{
	"semi": true,
	"tabWidth": 4,
	"printWidth": 80,
	"singleQuote": true,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"arrowParens": "avoid",
	"endOfLine": "auto",
	"jsxSingleQuote": true,
	"jsxBracketSameLine": false,
	"htmlWhitespaceSensitivity": "css",
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"useTabs": true,
	"bracketSameLine": false
}
